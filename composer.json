{
    "name": "felipemateus/laravel-iptv",
    "version": "2.0.1",
    "description": "This package controls the IPTV channel list and generates an m3u8 file using laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Felipe Mateus",
            "email": "contato@felipefm32.com.br"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.54"
	},
	"autoload": {
        "psr-4": {
			"FelipeMateus\\IPTVChannels\\":"src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FelipeMateus\\IPTVChannels\\IPTVProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
